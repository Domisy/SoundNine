<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.ToggleButton")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled"/>
		<s:State name="down"/>
		<s:State name="over"/>
		<s:State name="up"/>
		<s:State name="downAndSelected"/>
		<s:State name="disabledAndSelected"/>
		<s:State name="upAndSelected"/>
		<s:State name="overAndSelected"/>
	</s:states>
	
	<!-- SkinParts
	name=iconDisplay, type=spark.primitives.BitmapImage, required=false
	name=labelDisplay, type=spark.core.IDisplayText, required=falsetop="0" right="0" bottom="0" left="0"
	-->
	
	<s:BorderContainer cornerRadius="3" width="100%" height="100%">
		<s:Rect id="rect" alpha="1" radiusX="0" radiusY="0" width="100%" height="100%" >
			<s:fill>
				<s:SolidColor color="0x616161"/>
			</s:fill>	
		</s:Rect>	
		
		<s:borderStroke>
			<s:SolidColorStroke
				color="black"
				weight="1">
			</s:SolidColorStroke>
		</s:borderStroke>
		
	</s:BorderContainer>
	

	<s:BitmapImage id="iconDisplay" width="100%" height="100%"/>
	
	
</s:Skin>
